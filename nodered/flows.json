    {
        "id": "36c52e85636e223b",
        "type": "function",
        "z": "35663338d3b3c010",
        "name": "Humidity Data",
        "func": "var value = parseFloat(msg.humidity);\n\n// Déterminez le statut basé sur l'humidité\nvar status;\nif (value < 20) status = \"Very Dry\";\nelse if (value < 40) status = \"Dry\";\nelse if (value < 60) status = \"Normal\";\nelse if (value < 80) status = \"Humid\";\nelse status = \"Very Humid\";\n\nmsg.payload = {\n    value: value,\n    humidity: value,\n    unit: \"%\",\n    status: status,\n    timestamp: new Date().toLocaleString()\n};\nreturn msg;",
        "outputs": 1,
