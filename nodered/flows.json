    {
        "id": "b393ca08c3364dca",
        "type": "function",
        "z": "35663338d3b3c010",
        "name": "Temperature Data",
        "func": "var value = parseFloat(msg.payload);\n\n// Déterminez le statut basé sur la température\nvar status;\nif (value < 10) status = \"Very Cold\";\nelse if (value < 18) status = \"Cold\";\nelse if (value < 25) status = \"Normal\";\nelse if (value < 30) status = \"Warm\";\nelse status = \"Hot\";\n\nmsg.payload = {\n    value: value,\n    temperature: value,\n    unit: \"°C\",\n    status: status,\n    timestamp: new Date().toLocaleString()\n};\nreturn msg;",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
